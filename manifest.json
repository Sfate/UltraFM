{
  "background": {"page": "background.html"},
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "description": "Listen fav station from browser. Tiny extention that allows you to listen fav radio from Chrome browser.",
  "icons": {
    "128": "images/icon_128.png",
    "16": "images/icon.png",
    "48": "images/icon_48.png"
  },
  "name": "UltraFM",
  "manifest_version": 2,
  "minimum_chrome_version": "26.0",
  "offline_enabled": false,
  "permissions": [
    "http://s3.radioheart.ru:8015/",
    "http://ws.audioscrobbler.com/",
    "tabs"
  ],
  "options_page": "options.html",
  "version": "0.2.0.1"
}
